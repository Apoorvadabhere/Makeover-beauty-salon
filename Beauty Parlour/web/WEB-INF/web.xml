<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>LoginTask</servlet-name>
        <servlet-class>LoginTask</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationTask</servlet-name>
        <servlet-class>RegistrationTask</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutTask</servlet-name>
        <servlet-class>LogoutTask</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bookedslots</servlet-name>
        <servlet-class>bookedslots</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookTask</servlet-name>
        <servlet-class>BookTask</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mybooktask</servlet-name>
        <servlet-class>mybooktask</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RepeatEmail</servlet-name>
        <servlet-class>RepeatEmail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginTask</servlet-name>
        <url-pattern>/LoginTask</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationTask</servlet-name>
        <url-pattern>/RegistrationTask</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutTask</servlet-name>
        <url-pattern>/LogoutTask</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bookedslots</servlet-name>
        <url-pattern>/bookedslots</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookTask</servlet-name>
        <url-pattern>/BookTask</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mybooktask</servlet-name>
        <url-pattern>/mybooktask</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RepeatEmail</servlet-name>
        <url-pattern>/RepeatEmail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
